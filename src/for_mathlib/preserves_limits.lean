  (preserves_kernel.iso F f‚ÇÅ).inv ‚â´ F.map (kernel.map _ _ iX iY e) =
    kernel.map _ _ (F.map iX) (F.map iY) (by simp_rw [‚Üê F.map_comp, e]) ‚â´
      (preserves_kernel.iso F f‚ÇÇ).inv :=
begin
